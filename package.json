{
  "name": "office-addin-taskpane-react-js",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/OfficeDev/Office-Addin-TaskPane-React-JS.git"
  },
  "license": "MIT",
  "config": {
    "app-to-debug": "outlook",
    "app-type-to-debug": "desktop",
    "dev-server-port": 3000
  },
  "scripts": {
    "build": "node generate-config.js production && webpack -p --mode production --https false",
    "build:test": "node generate-config.js production && webpack --mode development --https false",
    "build:dev": "node generate-config.js localhost && webpack --mode development --https false",
    "dev-server": "node generate-config.js localhost && webpack-dev-server --mode development",
    "dev-server:linux": "node generate-config.js localhost && webpack-dev-server --mode development --https --cert $HOME/.office-addin-dev-certs/localhost.crt --cacert $HOME/.office-addin-dev-certs/ca.cert --key $HOME/.office-addin-dev-certs/localhost.key",
    "start": "office-addin-debugging start manifest/manifest-localhost.xml",
    "start:desktop": "office-addin-debugging start manifest/manifest-localhost.xml desktop",
    "start:web": "office-addin-debugging start manifest/manifest-localhost.xml web",
    "stop": "office-addin-debugging stop manifest/manifest-localhost.xml",
    "validate": "office-toolbox validate -m manifest/manifest-localhost.xml",
    "watch": "webpack --mode development --watch"
  },
  "dependencies": {
    "@microsoft/office-js-helpers": "^1.0.2",
    "@sentry/browser": "^5.6.3",
    "axios": "^0.19.0",
    "core-js": "^3.1.4",
    "es6-promise": "^4.2.6",
    "jwt-decode": "^2.2.0",
    "moment": "^2.24.0",
    "office-ui-fabric-react": "^6.139.0",
    "prop-types": "latest",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "readline-sync": "^1.4.9"
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@types/es6-collections": "^0.5.29",
    "@types/es6-promise": "0.0.32",
    "@types/office-js": "^1.0.9",
    "@types/office-runtime": "^1.0.8",
    "@types/react": "^16.8.3",
    "@types/react-dom": "^16.8.1",
    "@types/react-hot-loader": "^4.1.0",
    "@types/webpack": "4.4.22",
    "@types/webpack-dev-server": "3.1.1",
    "babel-loader": "^8.0.6",
    "babel-polyfill": "^6.26.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^2.1.1",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "moment-locales-webpack-plugin": "^1.0.7",
    "office-addin-debugging": "^2.3.0",
    "office-addin-dev-certs": "^1.3.3",
    "office-toolbox": "^0.1.1",
    "postcss-loader": "3.0.0",
    "react-hot-loader": "^3.1.3",
    "react-modal": "^3.9.1",
    "reactjs-popup": "^1.4.2",
    "source-map-loader": "^0.2.4",
    "style-loader": "0.23.0",
    "ts-loader": "^6.0.2",
    "typescript": "^3.5.1",
    "webpack": "^4.33.0",
    "webpack-bundle-analyzer": "^3.4.1",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.5.1"
  }
}
